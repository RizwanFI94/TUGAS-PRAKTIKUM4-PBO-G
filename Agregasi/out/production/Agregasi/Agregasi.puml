@startuml
'https://plantuml.com/class-diagram

class Prodi {
    - String kodeProdi
    - String namaProdi
    - Mahasiswa[] mhs

    + Prodi(kodeJurusan, namaJurusan)
    + setMahasiswa(Mahasiswa[] mhs)
    + daftarMahasiswa()
}

class Mahasiswa {
    - String NPM
    - String namaMahasiswa

    + Mahasiswa(NPM, namaMahasiswa)
    + getNPM():String
    + getNamaMahasiswa():String
}

Prodi "1" o-- "*" Mahasiswa

@enduml